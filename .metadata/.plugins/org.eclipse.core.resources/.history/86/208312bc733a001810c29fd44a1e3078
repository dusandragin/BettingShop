package model;

import java.io.Serializable;
import javax.persistence.*;

/**
 * The primary key class for the liga database table.
 * 
 */
@Embeddable
public class LigaPK implements Serializable {
	//default serial version id, required for serializable classes.
	private static final long serialVersionUID = 1L;

	private int idLiga;

	@Column(insertable=false, updatable=false)
	private int kategorija_idKategorija;

	public LigaPK() {
	}
	public int getIdLiga() {
		return this.idLiga;
	}
	public void setIdLiga(int idLiga) {
		this.idLiga = idLiga;
	}
	public int getKategorija_idKategorija() {
		return this.kategorija_idKategorija;
	}
	public void setKategorija_idKategorija(int kategorija_idKategorija) {
		this.kategorija_idKategorija = kategorija_idKategorija;
	}

	public boolean equals(Object other) {
		if (this == other) {
			return true;
		}
		if (!(other instanceof LigaPK)) {
			return false;
		}
		LigaPK castOther = (LigaPK)other;
		return 
			(this.idLiga == castOther.idLiga)
			&& (this.kategorija_idKategorija == castOther.kategorija_idKategorija);
	}

	public int hashCode() {
		final int prime = 31;
		int hash = 17;
		hash = hash * prime + this.idLiga;
		hash = hash * prime + this.kategorija_idKategorija;
		
		return hash;
	}
}