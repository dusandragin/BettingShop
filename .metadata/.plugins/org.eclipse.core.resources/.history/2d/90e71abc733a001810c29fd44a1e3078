package model;

import java.io.Serializable;
import javax.persistence.*;
import java.util.Date;


/**
 * The persistent class for the tiket database table.
 * 
 */
@Entity
@NamedQuery(name="Tiket.findAll", query="SELECT t FROM Tiket t")
public class Tiket implements Serializable {
	private static final long serialVersionUID = 1L;

	@EmbeddedId
	private TiketPK id;

	private byte dobitan;

	private double isplata;

	private String koeficijent;

	private double uplata;

	@Temporal(TemporalType.TIMESTAMP)
	private Date vremeDatum;

	//bi-directional many-to-one association to Klijent
	@ManyToOne
	@JoinColumn(name="Klijent_idKlijeta1")
	private Klijent klijent;

	public Tiket() {
	}

	public TiketPK getId() {
		return this.id;
	}

	public void setId(TiketPK id) {
		this.id = id;
	}

	public byte getDobitan() {
		return this.dobitan;
	}

	public void setDobitan(byte dobitan) {
		this.dobitan = dobitan;
	}

	public double getIsplata() {
		return this.isplata;
	}

	public void setIsplata(double isplata) {
		this.isplata = isplata;
	}

	public String getKoeficijent() {
		return this.koeficijent;
	}

	public void setKoeficijent(String koeficijent) {
		this.koeficijent = koeficijent;
	}

	public double getUplata() {
		return this.uplata;
	}

	public void setUplata(double uplata) {
		this.uplata = uplata;
	}

	public Date getVremeDatum() {
		return this.vremeDatum;
	}

	public void setVremeDatum(Date vremeDatum) {
		this.vremeDatum = vremeDatum;
	}

	public Klijent getKlijent() {
		return this.klijent;
	}

	public void setKlijent(Klijent klijent) {
		this.klijent = klijent;
	}

}